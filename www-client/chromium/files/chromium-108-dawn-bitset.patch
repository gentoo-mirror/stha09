From e6208597ac08e80a744b79ec1ac272601e9fe4a0 Mon Sep 17 00:00:00 2001
From: Stephan Hartmann <stha09@googlemail.com>
Date: Thu, 29 Sep 2022 11:21:37 +0000
Subject: [PATCH] libstdc++: replace bit_set::operator!= in ityp_bitset

C++20 removed the operator.
---
 third_party/dawn/src/dawn/common/ityp_bitset.h | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/third_party/dawn/src/dawn/common/ityp_bitset.h b/third_party/dawn/src/dawn/common/ityp_bitset.h
index d259d3a..d0e8e3d 100644
--- a/third_party/dawn/src/dawn/common/ityp_bitset.h
+++ b/third_party/dawn/src/dawn/common/ityp_bitset.h
@@ -60,7 +60,7 @@ class bitset : private std::bitset<N> {
     }
 
     bool operator!=(const bitset& other) const noexcept {
-        return Base::operator!=(static_cast<const Base&>(other));
+        return !Base::operator==(static_cast<const Base&>(other));
     }
 
     bitset& operator&=(const bitset& other) noexcept {
-- 
